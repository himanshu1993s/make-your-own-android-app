<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/RelativeLayout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="center"
    android:background="#ffffff"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
     >

    <TableRow
        android:id="@+id/TableRow01"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="10dp"
        android:layout_weight="0.00"
        android:gravity="center" >

        <Button
            android:id="@+id/auto1"
            android:layout_width="125dp"
            android:layout_height="100dp"
            android:background="@drawable/button2" 
            android:gravity="center"
            android:text="Terranot" />

        <Button
            android:id="@+id/auto2"
            android:layout_width="125dp"
            android:layout_height="100dp"
            android:layout_marginLeft="35dp"
            android:background="@drawable/button2"
            android:text="BlackHawk" />

    </TableRow>

    <TableRow
        android:id="@+id/tableRow1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/TableRow01"
        android:layout_marginTop="10dp"
        android:layout_weight="0.00"
        android:gravity="center" >

        <Button
            android:id="@+id/auto3"
            android:layout_width="125dp"
            android:layout_height="100dp"
            android:background="@drawable/button2"
            android:text="Entrant" />

        <Button
            android:id="@+id/auto4"
            android:layout_width="125dp"
            android:layout_height="100dp"
            android:layout_marginLeft="35dp"
            android:background="@drawable/button2"
            android:text="Junky-ard" />

    </TableRow>

    <TableRow
        android:id="@+id/tableRow2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/tableRow1"
        android:layout_marginTop="10dp"
        android:layout_weight="0.00"
        android:gravity="center" >

        <Button
            android:id="@+id/auto5"
            android:layout_width="125dp"
            android:layout_height="100dp"
            android:background="@drawable/button2"
            android:text="Kurukshetra" />

        <Button
            android:id="@+id/auto6"
            android:layout_width="125dp"
            android:layout_height="100dp"
            android:layout_marginLeft="35dp"
            android:background="@drawable/button2"
            android:text="Renaisance" />

    </TableRow>

    <TableRow
        android:id="@+id/TableRow02"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/tableRow2"
        android:layout_below="@+id/tableRow2"
        android:layout_marginTop="10dp"
        android:layout_weight="0.00"
        android:gravity="center" >

        <Button
            android:id="@+id/bb"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:background="@drawable/bb"
            android:ems="20"
            android:text="" />

    </TableRow>

    <TableRow
        android:id="@+id/tableRow3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/TableRow02"
        android:layout_below="@+id/TableRow02"
        android:layout_marginLeft="82dp"
        android:layout_marginTop="21dp" >
    </TableRow>

</RelativeLayout>